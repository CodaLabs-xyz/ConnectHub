# Deployment Configuration
# Copy this to .env and fill in your values

# Your deployer private key (with 0x prefix)
PRIVATE_KEY=0xyour_private_key_here

# Network to deploy to (base, base-sepolia, celo, celo-sepolia)
NETWORK=celo-sepolia

# Self Protocol Configuration
# Scope seed - must match frontend NEXT_PUBLIC_SELF_SCOPE
SCOPE_SEED=farcaster-miniapp-template

# Minimum age requirement (in years)
MINIMUM_AGE=18

# Self Protocol Hub Addresses
# Base Mainnet Hub (DO NOT CHANGE)
BASE_HUB_ADDRESS=0x0000000000000000000000000000000000000000 # Update with actual Base mainnet hub

# Base Sepolia Hub (DO NOT CHANGE)
BASE_SEPOLIA_HUB_ADDRESS=0x16ECBA51e18a4a7e61fdC417f0d47AFEeDfbed74

# Celo Mainnet Hub (DO NOT CHANGE)
CELO_HUB_ADDRESS=0xe57F4773bd9c9d8b6Cd70431117d353298B9f5BF

# Celo Sepolia (Alfajores) Hub (DO NOT CHANGE)
CELO_SEPOLIA_HUB_ADDRESS=0x16ECBA51e18a4a7e61fdC417f0d47AFEeDfbed74

# BaseScan API Key for contract verification
BASESCAN_API_KEY=your_basescan_api_key_here

# CeloScan API Key for contract verification
CELOSCAN_API_KEY=your_celoscan_api_key_here

# OFAC Compliance
OFAC_ENABLED=false

# Excluded Countries (3-letter ISO codes, comma-separated)
# Example: USA,CAN,GBR
EXCLUDED_COUNTRIES=
